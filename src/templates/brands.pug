-
  const brandsList = [
    {
      image: '@/assets/img/brands/brand-1.svg',
      alt: 'brand',
    },
    {
      image: '@/assets/img/brands/brand-2.svg',
      alt: 'brand',
    },
    {
      image: '@/assets/img/brands/brand-3.svg',
      alt: 'brand',
    },
    {
      image: '@/assets/img/brands/brand-4.svg',
      alt: 'brand',
    },
    {
      image: '@/assets/img/brands/brand-5.svg',
      alt: 'brand',
    },
    {
      image: '@/assets/img/brands/brand-6.svg',
      alt: 'brand',
    },
  ];

section.brands
  ._container
    .brands__wrapper
      h2.brands__title Popular brands we work with

      .brands__main
        .brands-slider.swiper
          .swiper-wrapper
            - for (let i=0; i<2; i++)
              each item in brandsList
                .swiper-slide
                  .brands-slider__item
                    img(src=require(`${item.image}`), alt=`${item.alt}`)

      .brands__footer
        .brands-slider-pagination
